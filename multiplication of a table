import java.util.*;
class multiplication{
    public static void main(String[] args){
        Scanner sc =new Scanner(System.in);
        System.out.print("the number is"+" ");
         int a =sc.nextInt();
        num(a);
}

static void num(int n){
    for(int i=1;i<=10;i++)
        {
    System.out.println(i + " "+"*" +" "+ n + " "+"="+i*n );
}
}
}
